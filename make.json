{
	"info": {
		"name": "ForestryPE",
		"author": "DDCompany",
		"version": "7.0",
		"description": "Need Wood? Get Forestry!",
		"clientOnly": false
	},
	"project": {
		"compiledLibraries": true,
		"useReferences": true
	},
	"api": "CoreEngine",
	"resources": [
		{
			"path": "assets/res",
			"type": "resource_directory"
		},
		{
			"path": "assets/gui",
			"type": "gui"
		}
	],
	"sources": [
		{
			"source": "dev",
			"target": "main.js",
			"type": "main",
			"language": "typescript"
		},
		{
			"source": "launcher.js",
			"type": "launcher",
			"language": "javascript"
		},
		{
			"source": "lib/*",
			"type": "library",
			"language": "javascript"
		}
	],
	"additional": [
		{
			"source": "assets/root/*",
			"targetDir": "."
		}
	],
	"pushTo": "/storage/emulated/0/games/horizon/packs/Inner_Core/innercore"
}
